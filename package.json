{
	"name": "workout-app",
	"version": "1.0.0",
	"main": "index.js",
	"author": "Kotenko Sergey",
	"license": "MIT",
	"scripts": {
		"server": "nodemon back/server",
		"client": "yarn --cwd front start",
		"dev": "concurrently --kill-other-on-fail \"yarn server\" \"yarn client\"",
		"start": "concurrently --kill-other-on-fail \"yarn server\" \"yarn client\"",
		"build": "cd front && npm run build",
		"install-client": "cd front && npm install",
		"heroku-postbuild": "npm run install-client && npm run build"
	},
	"type": "module",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.6.5",
		"morgan": "^1.10.0"
	},
	"devDependencies": {
		"concurrently": "^7.4.0",
		"nodemon": "^2.0.20"
	}
}
